{
  "compilerOptions": {
    "module": "esnext",
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@notesync/functions": ["./packages/integrations/notion/notesync/notesync.functions.ts"],
      "@notesync/schema": ["./packages/integrations/notion/notesync/notesync.schema.ts"],
      "@notesync/embed": ["./packages/integrations/notion/notesync/notesync.embed.ts"],
      "@/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": ["node_modules"]
}